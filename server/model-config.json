{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mLab"
  },
  "AccessToken": {
    "dataSource": "mLab",
    "public": false
  },
  "ACL": {
    "dataSource": "mLab",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mLab",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mLab",
    "public": false
  },
  "Organization": {
    "dataSource": "mLab",
    "public": true
  },
  "Donner": {
    "dataSource": "mLab",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": true,
          "create": true,
          "sampleRemoteMethod": true
        }
      }
    }
  },
  "Contact": {
    "dataSource": "mLab",
    "public": true
  },
  "DonnerReview": {
    "dataSource": "mLab",
    "public": true
  },
  "OrganizationReview": {
    "dataSource": "mLab",
    "public": true
  },
  "DonationRequest": {
    "dataSource": "mLab",
    "public": false
  },
  "OneTimeRequest": {
    "dataSource": "mLab",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "create": true,
          "sampleRemoteMethod": true
        }
      }
    }
  },
  "PermanentRequest": {
    "dataSource": "mLab",
    "public": true
  },
  "DonationResponse": {
    "dataSource": "mLab",
    "public": true
  },
  "Product": {
    "dataSource": "mLab",
    "public": true
  },
  "Unit": {
    "dataSource": "mLab",
    "public": true
  }
}
